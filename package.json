{
  "name": "@kanthgithub/shib-dao-contracts",
  "description": "solidity contracts for shib voting dao",
  "version": "1.0.0",
  "author": {
    "name": "kanthgithub",
    "url": "https://github.com/kanthgithub"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.1"
  },
  "devDependencies": {
    "@prb/test": "^0.6.4",
    "forge-std": "github:foundry-rs/forge-std#v1.7.5",
    "prettier-plugin-solidity": "^1.1.3",
    "prettier": "^3.0.0",
    "solhint": "^3.6.2"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "forge",
    "foundry",
    "smart-contracts",
    "solidity",
    "template"
  ],
  "private": true,
  "scripts": {
    "clean": "rm -rf cache out",
